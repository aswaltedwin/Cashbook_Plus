{
  "version": 3,
  "name": "cashbook-plus",
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/api/(.*)", "dest": "/api/main.py" },
    { "src": "/(.*)", "dest": "/api/main.py" }
  ]
}
