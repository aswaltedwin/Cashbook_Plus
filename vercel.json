{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/static/index.html"
      },
      {
        "src": "/dashboard",
        "dest": "/static/dashboard.html"
      },
      {
        "src": "/cashbooks",
        "dest": "/static/cashbooks.html"
      },
      {
        "src": "/login",
        "dest": "/static/login.html"
      },
      {
        "src": "/register",
        "dest": "/static/register.html"
      },
      {
        "src": "/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  